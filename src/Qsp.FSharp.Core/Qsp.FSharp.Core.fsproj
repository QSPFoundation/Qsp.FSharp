<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <!-- `paket install` set `AutoGenerateBindingRedirects` is true. -->
    <!-- I don't know how disable it. -->
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
  </PropertyGroup>
  <ItemGroup>
    <None Include="App.config" />
    <Compile Include="Scope.fs" />
    <Compile Include="Defines.fs" />
    <Compile Include="Ast.fs" />
    <Compile Include="Show.fs" />
    <Compile Include="Parsers\Tokens.fs" />
    <Compile Include="Parsers\ParserGeneric.fs" />
    <Compile Include="Parsers\Value.fs" />
    <Compile Include="Parsers\Expr.fs" />
    <Compile Include="Parsers\Statement.fs" />
    <Compile Include="Parsers\Statements.fs" />
    <Compile Include="Parsers\Location.fs" />
    <Compile Include="Parsers\Document.fs" />
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>